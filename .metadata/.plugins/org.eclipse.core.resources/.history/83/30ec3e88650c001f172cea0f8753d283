package app;

import java.util.List;

import Entities.HoaDon;
import Entities.NhanVien;
import Entities.ThucDon;
import Services.HoaDon_Service;
import dao.NhanVien_DAO;
import dao.ThucDon_DAO;

public class readThucDon {
	 public static void main(String[] args) {
	        ThucDon_DAO thucDonDAO = new ThucDon_DAO();
	        List<ThucDon> thucDons = thucDonDAO.getAllThucDon();
	        for (ThucDon thucDon : thucDons) {
	            System.out.println(thucDon);
	        }
	        thucDonDAO.close();
		    
		    	NhanVien_DAO nvdao = new NhanVien_DAO();
		        List<NhanVien> nvs = nvdao.getAllNhanVien();
		        for (NhanVien nv : nvs) {
		            System.out.println(nv);
		        }
		        thucDonDAO.close();
		 
		 HoaDon_Service hddao = new HoaDon_Service();
	        List<HoaDon> hd = hddao.getAllThucDon();
	        for (HoaDon nv : hd) {
	            System.out.println(nv);
	        }
	        thucDonDAO.close();
		    
	    }
}
